<div class="map-control-element">
    <div class="layer-control-head">
        <div class="layer-toggle">
            {{#if isVisible}}
                <i class="fa fa-check-square-o"></i>
            {{else}}
                <i class="fa fa-square-o"></i>
            {{/if}}
        </div>
        <div class="layer-title">{{label}}</div>
    </div>
    <div class="layer-control-body">
    </div>
</div>
